{
  "name": "joker",
  "version": "0.1.4",
  "description": "Application that tells joke",
  "main": "index.js",
  "scripts": {
    "test": "npm run test-unit",
    "test-unit": "jest",
    "format": "prettier -w src/**/*.js",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nyolarraklay/joker.git"
  },
  "keywords": [
    "website"
  ],
  "author": "nyolosorio",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nyolarraklay/joker/issues"
  },
  "homepage": "https://github.com/nyolarraklay/joker#readme",
  "type": "module",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "eslint": "^8.52.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.4.3",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.2.0",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3"
  },
  "dependencies": {
    "give-me-a-joke": "^0.5.1"
  }
}
